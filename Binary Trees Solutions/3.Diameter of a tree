int ma;
    int height(Node * root){
        if(root==NULL)
        return 0;
        int h1=height(root->left);
        int h2=height(root->right);
        ma=max(ma,h1+h2+1);
        return max(h1,h2)+1;
    }
    int diameter(Node* root) {
        // Your code here
       ma=INT_MIN;
       int x=height(root);
       return ma;
    }
